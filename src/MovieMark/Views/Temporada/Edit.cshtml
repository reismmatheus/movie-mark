@model MovieMark.Models.TemporadaViewModels.TemporadaEditViewModel

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form asp-route="Edit" method="post" id="form-create">

    <input asp-for="Id" type="hidden" />

    <div class="form-group">
        <label asp-for="Nome">Nome da Temporada</label>
        <input asp-for="Nome" type="text" class="form-control" id="" value="@Model.Nome">
    </div>
    <button type="submit" class="btn btn-primary">Atualizar</button>
</form>

